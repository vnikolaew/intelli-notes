{
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS",
      "paths": {
        "@/*": [
          "./*"
        ]
      }
    }
  },
  "display": "Next.js",
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "baseUrl": ".",
    "target": "ES2020",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "strict": false,
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "next.config.js",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
